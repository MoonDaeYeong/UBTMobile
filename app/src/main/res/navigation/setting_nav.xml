<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/setting_nav"
    app:startDestination="@id/settingHomeFragment">

    <fragment
        android:id="@+id/settingHomeFragment"
        android:name="com.nsdevil.ubtmobilev3.ui.setting.SettingHomeFragment"
        android:label="fragment_setting_home"
        tools:layout="@layout/fragment_setting_home" >
        <action
            android:id="@+id/action_settingHomeFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_settingHomeFragment_to_passChangeFragment"
            app:destination="@id/passChangeFragment" />
    </fragment>
    <fragment
        android:id="@+id/passChangeFragment"
        android:name="com.nsdevil.ubtmobilev3.ui.setting.PassChangeFragment"
        android:label="fragment_pass_change"
        tools:layout="@layout/fragment_pass_change" />
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.nsdevil.ubtmobilev3.ui.setting.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" />
</navigation>